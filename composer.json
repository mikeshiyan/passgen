{
    "name": "shiyan/passgen",
    "description": "Password generating Symfony Console application.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "consolidation/annotated-command": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "Shiyan\\Passgen\\": "src/"
        }
    },
    "bin": [
        "bin/passgen"
    ],
    "keywords": ["password", "generator", "php", "oop", "console"],
    "support": {
        "source": "https://github.com/mikeshiyan/passgen",
        "issues": "https://github.com/mikeshiyan/passgen/issues"
    },
    "authors": [
        {
            "name": "Mike Shiyan",
            "homepage": "https://github.com/mikeshiyan"
        }
    ],
    "suggest": {
        "consolidation/cgr": "Isolates the composer library from other globally-installed tools"
    }
}
